{
  "env": {
    "minix.include": "${workspaceFolder}/../../minix-src/include"
  },
  "configurations": [{
    "name": "Minix",
    "compilerPath": "",
    "cStandard": "c11",
    "defines": ["__minix", "_MINIX ", "_NETBSD_SOURCE", "__i386__"],
    "includePath": [
      "${workspaceFolder}", "${env:minix.include}", "${workspaceFolder}/include"
    ],
    "browse": {
      "path": [
        "${workspaceFolder}", "${env:minix.include}", "${workspaceFolder}/include"
      ],
      "limitSymbolsToIncludedHeaders": false,
      "databaseFilename": ""
    }
  }],
  "version": 4
}